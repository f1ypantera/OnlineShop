@model PagedList.IPagedList<OnlineShop.Models.CarModel>
@using PagedList.Mvc;

@{
    ViewBag.Title = "List";
}

@foreach (var p in @Model)
{
<div>
    <h3>@p.NameModel  @p.NameManufacturer</h3>
    <p>@p.NameCategory</p>
    <p>@p.Description</p>
    <h4>@p.Price</h4>
</div>
}

Страница @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) из @Model.PageCount

@Html.PagedListPager(Model, page => Url.Action("Index", new { page }))

